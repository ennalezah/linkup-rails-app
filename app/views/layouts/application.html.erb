<!DOCTYPE html>
<html>
  <head>
    <title>LinkupRailsApp</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    
    <p class="alert alert-success"><%= notice if notice%></p>
    <p class="alert alert-danger"><%= alert if alert %></p>

    <%= render 'layouts/navbar' %>

    <%= yield %>
  </body>
</html>
